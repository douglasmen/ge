						width="535"
						height="16"
						key="textField-95"
						positionType="Float"/>
					<box topBorder="None" topBorderColor="#000000" leftBorder="None" leftBorderColor="#000000" rightBorder="None" rightBorderColor="#000000" bottomBorder="None" bottomBorderColor="#000000"/>
					<textElement verticalAlignment="Middle" isStyledText="true">
						<font/>
					</textElement>
				<textFieldExpression   class="java.lang.String"><![CDATA["O presente contrato poderá ser rescindido pelo <style isBold='true' pdfFontName='Helvetica-Bold'>CONTRATANTE</style>, antes do vencimento, desde que a <style isBold='true' pdfFontName='Helvetica-Bold'>ESCOLA</style> seja comunicada por escrito, com uma antecipação mínima de 30 (trinta) dias, <style isBold='true' pdfFontName='Helvetica-Bold'>sendo devido o pagamento desse período.</style> "]]></textFieldExpression>
				</textField>
				<textField isStretchWithOverflow="true" isBlankWhenNull="false" evaluationTime="Now" hyperlinkType="None"  hyperlinkTarget="Self" >
					<reportElement
						mode="Opaque"
